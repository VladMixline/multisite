{
  "name": "webproj",
  "version": "1.0.0",
  "description": "React + Vite + Node.js project",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "commit": "powershell -ExecutionPolicy Bypass -File scripts/auto-commit-push.ps1",
    "commit:msg": "powershell -ExecutionPolicy Bypass -File scripts/auto-commit-push.ps1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
